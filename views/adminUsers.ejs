<%- include('./partials/header'); %>

<div class="card login p-5" style="margin-top:80px;">
    <% if (responses.messageErr !== "") {%>
        <div class="alert alert-danger alert-dismissible fade show w-100" role="alert">
            <strong id="errRegister"><%= responses.messageErr %></strong>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    <% } %>
    <% if (responses.messageOK !== "") {%>
        <div class="alert alert-success alert-dismissible fade show w-100" role="alert">
            <strong id="errRegister"><%= responses.messageOK %></strong>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    <% } %>
    <h1 class="mb-4 mx-auto">Lista de usuarios</h1>
    <form action="/adminEditUser/<%= usuario.id%>" method="POST" enctype="multipart/form-data">
    </form>
        

</div>

<%- include('./partials/bottom'); %>