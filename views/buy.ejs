<%- include('./partials/header'); %>

    <div class="containerContact">
        <h2 class="name-login"> <img src="Images/BigLogo.png" style="padding-bottom: 9px;"> Sigma</h2>

        <div class="container">
            <div class="collapse multi-collapse hideMeBox" id="OtrosPanel">
                <%var i=0%>
                <%Car.forEach(el => {%>
                <div class="accordion " id="accordion">
                    <div class="card comentarios">
                        <div class="card-header" id="headingThree">
                            <h2 class="mb-0">
                                <button class="btn btn-link btn-block text-left" type="button" data-toggle="collapse"
                                    data-target="#product<%=i%>">
                                    <div class="row">
                                        <div class="col-sm">
                                            Nombre: <%=el.NombreEmpresa%>
                                        </div>
                                        <div class="col-sm">
                                            Tipo de Entidad: <%=el.TipoEntidad%>
                                        </div>
                                        <span class="btn" type=""><i class="far fa-trash-alt"></i></span>
                                    </div>
                                </button>
                            </h2>
                        </div>
    
                        <div id="product<%=i%>" class="collapse">
                            <div class="card-body ">
                                <div class="row">
                                    <div class="col-sm">
                                        Nombre: <%=el.Nombre%>
                                    </div>
                                    <div class="col-sm">
                                        Apellido: <%=el.Apellido%>
                                    </div>
                                    <div class="col-sm">
                                        RIF: <%=el.RIF%>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col-sm">
                                        Pais: <%=el.Pais%>
                                    </div>
                                    <div class="col-sm">
                                        Estado: <%=el.Estado%>
                                    </div>
                                    <div class="col-sm">
                                        Ciudad: <%=el.Ciudad%>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col-sm">
                                        Correo: <%=el.Email%>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col-sm">
                                        Comentario: <%=el.Comentario%>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <%i++%>
                <%});%>
            </div>
        </div>

    </div>

<%- include('./partials/bottom'); %>